{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components": ["components/index"],
      "@util*": ["utils/index"],
      "@store/*": ["store/*"],
      "@const": ["const/index"],
      "@styles/*": ["styles/*"],
      "@interfaces": ["interfaces/index"],
      "@models/*": ["../backend/models/*"]
    },
    "jsx": "react",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  }
}
